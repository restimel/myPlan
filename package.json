{
    "name": "myplan",
    "version": "0.15.3",
    "license": "MIT",
    "author": "Beno√Æt Mariat (https://github.com/restimel)",
    "contributors": [],
    "private": false,
    "homepage": "https://github.com/restimel/myPlan#readme",
    "bugs": {
        "url": "https://github.com/restimel/myPlan/issues"
    },
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "run-p type-check \"build:only {@}\" --",
        "build-all": "run-p type-check \"build:all {@}\" --",
        "preview": "vite preview",
        "build:all": "npm run build:only && npm run build:test",
        "build:only": "vite build",
        "build:test": "vite build --config vite.config.test.ts",
        "type-check": "vue-tsc --build",
        "lint": "eslint . --fix",
        "deploy": "npm run deployBuild",
        "deployBuild": "npm run build-all && git diff --quiet dist || (git clean -fd dist && git checkout -- dist && npm version patch && npm run build-all && git add dist && git commit -m 'update production version') && git subtree push --prefix dist origin gh-pages",
        "deployTest": "npm version prerelease --preid=test && npm run build:test && git add dist/test && git commit -m 'update test version' && git subtree push --prefix dist origin gh-pages"
    },
    "dependencies": {
        "@vueuse/core": "^14.1.0",
        "vue": "^3.5.26",
        "vue-i18n": "^11.2.8",
        "vue-router": "^4.6.4"
    },
    "devDependencies": {
        "@tsconfig/node22": "^22.0.5",
        "@types/node": "^25.0.3",
        "@vitejs/plugin-basic-ssl": "^2.1.0",
        "@vitejs/plugin-vue": "^6.0.3",
        "@vue/eslint-config-typescript": "^14.6.0",
        "@vue/tsconfig": "^0.8.1",
        "eslint": "^9.32.2",
        "eslint-plugin-vue": "~10.6.2",
        "jiti": "^2.6.1",
        "npm-run-all2": "^8.0.4",
        "typescript": "~5.9.3",
        "vite": "^7.3.0",
        "vite-plugin-vue-devtools": "^8.0.5",
        "vue-tsc": "^3.2.1"
    }
}
